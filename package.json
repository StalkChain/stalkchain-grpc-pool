{
  "name": "@stalkchain/grpc-pool",
  "version": "1.0.0",
  "description": "High-availability gRPC connection pooling module with active-active configuration, deduplication, and stale connection detection",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:pool": "pnpm run build && node test-pool.js",
    "example:solana": "pnpm run build && node -r ts-node/register examples/solana-tracker-example.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run test && pnpm run lint"
  },

  "dependencies": {
    "@triton-one/yellowstone-grpc": "^4.0.2",
    "dotenv": "^16.3.1",
    "eventemitter3": "^5.0.1",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "grpc",
    "solana",
    "yellowstone",
    "blockchain",
    "high-availability",
    "pooling",
    "deduplication",
    "stalkchain",
    "typescript",
    "nodejs"
  ],
  "author": "StalkChain Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StalkChain/stalkchain-grpc-pool.git"
  },
  "bugs": {
    "url": "https://github.com/StalkChain/stalkchain-grpc-pool/issues"
  },
  "homepage": "https://github.com/StalkChain/stalkchain-grpc-pool#readme"
}
